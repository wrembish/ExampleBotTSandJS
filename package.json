{
  "name": "examplebot",
  "version": "1.0.0",
  "description": "An example discord bot that contains both code in js and the ts equivelant",
  "main": "js/index.js",
  "scripts": {
    "prets-start": "npx tsc",
    "ts-start": "node ts/dist/index.js",
    "prets-dev": "ts-node ts/src/deploy-commands-dev.ts",
    "ts-dev": "nodemon ts/src/index.ts",
    "prets-deploy-comms": "npx tsc",
    "ts-deploy-comms": "node ts/dist/deploy-commands.js",
    "start": "node js/index.js",
    "predev": "node js/deploy-commands-dev.js",
    "dev": "nodemon js/index.js",
    "deploy-comms": "node js/deploy-commands.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wrembish/ExampleBotTSandJS"
  },
  "author": "William Rembish",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^14.3.0",
    "dotenv": "^16.0.1",
    "mongodb": "^4.9.0",
    "node-cron": "^3.0.2"
  },
  "devDependencies": {
    "@types/node-cron": "^3.0.2",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
